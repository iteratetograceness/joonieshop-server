{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Sebastian Rindom <skrindom@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "cross-env npm run clean && tsc -p tsconfig.json",
    "watch": "cross-env tsc --watch",
    "test": "cross-env jest",
    "seed": "cross-env medusa seed -f ./data/seed.json",
    "start": "cross-env npm run build && medusa start",
    "start:custom": "cross-env npm run build && node --preserve-symlinks index.js",
    "dev": "cross-env npm run build && medusa develop",
    "build:admin": "cross-env medusa-admin build"
  },
  "dependencies": {
    "@medusajs/admin": "^3.0.0",
    "@medusajs/cache-redis": "^1.8.3",
    "@medusajs/medusa": "^1.9.0",
    "@medusajs/medusa-cli": "^1.3.11",
    "awilix": "^8.0.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joonieshop-event-bus": "^1.0.1",
    "medusa-fulfillment-manual": "^1.1.37",
    "medusa-fulfillment-shippo": "^1.1.2",
    "medusa-interfaces": "^1.3.7",
    "medusa-payment-manual": "^1.0.23",
    "medusa-payment-paypal": "^4.0.0",
    "medusa-payment-stripe": "^1.1.53",
    "resend": "^0.15.1",
    "typeorm": "^0.3.11"
  },
  "keywords": [
    "sqlite",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.5",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@react-email/button": "0.0.8",
    "@react-email/container": "0.0.8",
    "@react-email/font": "0.0.1",
    "@react-email/head": "0.0.5",
    "@react-email/heading": "0.0.8",
    "@react-email/hr": "0.0.5",
    "@react-email/html": "0.0.4",
    "@react-email/preview": "0.0.6",
    "@react-email/section": "0.0.9",
    "@react-email/tailwind": "0.0.8",
    "@react-email/text": "0.0.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "mongoose": "^7.1.0",
    "react": "^18.2.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "patchedDependencies": {
      "@medusajs/medusa@1.9.0": "patches/@medusajs__medusa@1.9.0.patch"
    }
  }
}
